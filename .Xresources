!! URxvt Appearance
!URxvt.font: xft:Mono:pixelsize=14:antialias=true
!URxvt.font: xft:inconsolata:pixelsize=15:antialias=true
URxvt.font: xft:SourceCodePro:pixelsize=15:antialias=true
!URxvt.italicFont: xft:DejaVu Sans Mono:pixelsize=15, style=Book
!URxvt.boldFont: xft: 
!URxvt.italicFont: xft:DejaVu Sans Mono:pixelsize=15,style=Book
!URxvt.boldItalicfont: xft:DejaVu Sans Mono:pixelsize=15, style=Book
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 94x23
URxvt.internalBorder: 4
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: true 
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.iso14755: false

!! URxvt Functions
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.perl-ext-common: default,resize-font
URxvt.keysym.C-j: resize-font:smaller
URxvt.keysym.C-k: resize-font:bigger
URxvt.keysym.C-equal: resize-font:reset
URxvt.keysym.C-question: resize-font:show

!! URxvt Keybinds 
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007

URxvt.keysym.Alt-k: command:\033]720;1\007
URxvt.keysym.Alt-j: command:\033]721;1\007
